inherit ocaml

NAME="ocaml-duppy"
VERSION=0.5.1
RELEASE=1
CATEGORY="Libs"
SUMMARY="OCaml advanced scheduler library"
DESCRIPTION="ocaml-duppy is an advanced scheduler for Ocaml programmers."
HOMEPAGE="http://savonet.sourceforge.net/"
SRC_URI="mirror://sourceforge/savonet/${NAME}-${VERSION}.tar.gz"

src_compile() {
	lndirs
	cd ${B}
	cygconf
	cygmake -j1
}
